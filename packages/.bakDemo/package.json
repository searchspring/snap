{
	"name": "snap-preact-demo",
	"version": "1.0.1",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "rm -fr ./dist && webpack --mode=production --display-modules",
		"build:watch": "rm -fr ./dist && webpack --watch --mode=production",
		"dev": "webpack serve",
		"cypress": "cypress open --project tests",
		"format": "prettier --write \"**/*.{js,ts,json}\"",
		"lint": "eslint --fix \"**/*.{js,ts}\"",
		"test": "cypress run --project tests"
	},
	"author": "",
	"license": "ISC",
	"prettier": "@searchspring/prettier",
	"dependencies": {
		"@searchspring/snap-client-javascript": "file:../Client/Javascript",
		"@searchspring/snap-controller": "file:../Controller",
		"@searchspring/snap-event-manager": "file:../EventManager",
		"@searchspring/snap-logger": "file:../Logger",
		"@searchspring/snap-profiler": "file:../Profiler",
		"@searchspring/snap-store-mobx": "file:../Store/MobX",
		"@searchspring/snap-toolbox": "file:../Toolbox",
		"@searchspring/snap-url-manager": "file:../UrlManager",
		"mobx": "^6.1.8",
		"mobx-react": "^7.1.0",
		"preact": "^10.5.13"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"./package.json": [
			"prettier --write"
		],
		"*.{js,ts}": [
			"prettier --write",
			"eslint --fix"
		]
	}
}
