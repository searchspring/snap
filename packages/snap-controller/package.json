{
	"name": "@searchspring/snap-controller",
	"version": "0.0.6",
	"description": "Snap Controllers",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"author": "Searchspring",
	"license": "MIT",
	"scripts": {
		"build": "echo 'building snap-controller' && rm -rf ./dist && tsc && tsc -p tsconfig.cjs.json",
		"commit": "cz",
		"dev": "tsc --watch",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint 'src/**/*.{js,ts,tsx}'",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch",
		"preversion": "npm run lint && npm test && npm run build",
		"version": "standard-version",
		"postversion": "git push && git push --tags"
	},
	"dependencies": {
		"deepmerge": "^4.2.2"
	},
	"devDependencies": {
		"@searchspring/snap-event-manager": "0.0.16",
		"@searchspring/snap-store-mobx": "0.0.6",
		"@types/jest": "^26.0.22",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1",
		"standard-version": "^9.1.1",
		"ts-jest": "^26.5.4",
		"typescript": "^4.2.3",
		"@searchspring/snap-client-javascript": "^1.0.6",
		"@searchspring/snap-logger": "0.0.1",
		"@searchspring/snap-profiler": "0.0.12",
		"@searchspring/snap-url-manager": "0.0.15",
		"regenerator-runtime": "^0.13.7"
	},
	"sideEffects": false,
	"files": [
		"dist/**/*"
	]
}
