		"@babel/preset-react": "^7.18.6",
		"@babel/runtime": "^7.21.0",
		"@searchspring/snap-client": "^0.49.1",
		"@searchspring/snap-controller": "^0.49.1",
		"@searchspring/snap-event-manager": "^0.49.1",
		"@searchspring/snap-logger": "^0.49.1",
		"@searchspring/snap-profiler": "^0.49.1",
		"@searchspring/snap-store-mobx": "^0.49.1",
		"@searchspring/snap-tracker": "^0.49.1",
		"@searchspring/snap-url-manager": "^0.49.1",
		"@storybook/addon-actions": "6.4.22",
		"@storybook/addon-controls": "6.4.22",
		"@storybook/addon-docs": "6.4.22",
		"@storybook/addon-essentials": "6.4.22",
		"@storybook/addon-links": "6.4.22",
		"@storybook/builder-webpack5": "6.4.22",
