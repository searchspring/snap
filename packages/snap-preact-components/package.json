{
	"name": "@searchspring/snap-preact-components",
	"version": "0.49.1",
	"description": "Snap Preact Component Library",
	"author": "Searchspring",
	"license": "MIT",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"repository": "https://github.com/searchspring/snap",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/**/*",
		"README.md"
	],
	"scripts": {
		"build": "rm -rf ./dist && tsc && tsc -p tsconfig.cjs.json",
		"build:docs": "build-storybook -o ./docs",
		"dev": "tsc --watch",
		"format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
		"lint": "eslint 'src/components/**/*.{js,jsx,ts,tsx}'",
		"storybook": "start-storybook -p 6006",
		"cypress": "cypress open --project tests",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"dependencies": {
		"@cypress/react": "^8.0.0",
		"@emotion/react": "11.9.0",
		"@searchspring/snap-toolbox": "^0.49.1",
		"classnames": "^2.3.2",
		"cypress": "^12.15.0",
		"cypress-wait-until": "^1.7.2",
		"deepmerge": "4.3.1",
		"dequal": "2.0.3",
		"mobx-react-lite": "3.4.0",
		"react-ranger": "2.1.0",
		"swiper": "9.0.5"
	},
	"peerDependencies": {
		"preact": "10.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.21.0",
