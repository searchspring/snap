		"@storybook/preact": "6.4.22",
		"@storybook/theming": "6.4.22",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/preact": "2.0.1",
		"@testing-library/user-event": "13.5.0",
		"@types/react": "16.14.28",
		"@types/react-ranger": "2.0.1",
		"babel-loader": "^9.1.2",
		"mobx-react": "7.5.0",
		"preact": "10.9.0",
		"react": "16.14.0",
		"react-dom": "16.14.0",
		"ts-loader": "9.4.2",
		"webpack": "^5.77.0",
