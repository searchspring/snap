{
	"name": "@searchspring/snap-preact-components",
	"version": "0.67.3",
	"description": "Snap Preact Component Library",
	"author": "Searchspring",
	"license": "MIT",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"repository": "https://github.com/searchspring/snap",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/**/*",
		"README.md"
	],
	"scripts": {
		"build": "rm -rf ./dist && tsc && tsc -p tsconfig.cjs.json",
		"build:docs": "build-storybook -o ./docs",
		"dev": "tsc --watch",
		"format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
		"lint": "eslint 'src/components/**/*.{js,jsx,ts,tsx}'",
		"storybook": "start-storybook -p 6006",
		"cypress": "cypress open --project tests",
		"cypress:headless": "cypress run --component --project tests",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"dependencies": {
		"@cypress/react": "^8.0.0",
		"@emotion/react": "11.9.0",
		"@searchspring/snap-toolbox": "0.67.3",
		"classnames": "^2.3.2",
		"cypress": "^13.7.1",
		"cypress-wait-until": "^1.7.2",
		"deepmerge": "4.3.1",
		"dequal": "2.0.3",
		"mobx-react": "7.6.0",
		"react-ranger": "2.1.0",
		"swiper": "11.0.7"
	},
	"peerDependencies": {
		"mobx": "6.9.0",
		"preact": "10.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.21.0",
		"@babel/plugin-transform-react-jsx": "^7.21.0",
		"@babel/plugin-transform-runtime": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@babel/runtime": "^7.21.0",
		"@searchspring/snap-client": "0.67.3",
		"@searchspring/snap-controller": "0.67.3",
		"@searchspring/snap-event-manager": "0.67.3",
		"@searchspring/snap-logger": "0.67.3",
		"@searchspring/snap-profiler": "0.67.3",
		"@searchspring/snap-store-mobx": "0.67.3",
		"@searchspring/snap-tracker": "0.67.3",
		"@searchspring/snap-url-manager": "0.67.3",
		"@storybook/addon-actions": "6.4.22",
		"@storybook/addon-controls": "6.4.22",
		"@storybook/addon-docs": "6.4.22",
		"@storybook/addon-essentials": "6.4.22",
		"@storybook/addon-links": "6.4.22",
		"@storybook/builder-webpack5": "6.4.22",
		"@storybook/manager-webpack5": "6.4.22",
		"@storybook/preact": "6.4.22",
		"@storybook/theming": "6.4.22",
		"@testing-library/preact": "3.2.3",
		"@testing-library/user-event": "13.5.0",
		"@types/react": "16.14.28",
		"@types/react-ranger": "2.0.1",
		"@types/scheduler": "0.16.8",
		"babel-jest": "^29.7.0",
		"preact": "10.9.0",
		"react": "16.14.0",
		"react-dom": "16.14.0",
		"ts-loader": "9.4.2",
		"webpack": "^5.77.0",
		"webpack-cli": "^5.0.1",
		"webpack-merge": "^5.8.0"
	},
	"sideEffects": false
}
