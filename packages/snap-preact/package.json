{
	"name": "@searchspring/snap-preact",
	"version": "0.28.0",
	"description": "Snap Preact",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"author": "Searchspring",
	"license": "MIT",
	"repository": "https://github.com/searchspring/snap",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rm -rf ./dist && tsc && tsc -p tsconfig.cjs.json",
		"build:docs": "typedoc --out docs src/index.ts",
		"dev": "tsc --watch",
		"format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
		"lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"dependencies": {
		"@searchspring/snap-client": "^0.28.0",
		"@searchspring/snap-controller": "^0.28.0",
		"@searchspring/snap-event-manager": "^0.28.0",
		"@searchspring/snap-logger": "^0.28.0",
		"@searchspring/snap-preact-components": "^0.28.0",
		"@searchspring/snap-profiler": "^0.28.0",
		"@searchspring/snap-store-mobx": "^0.28.0",
		"@searchspring/snap-toolbox": "^0.28.0",
		"@searchspring/snap-tracker": "^0.28.0",
		"@searchspring/snap-url-manager": "^0.28.0",
		"deepmerge": "4.2.2",
		"intersection-observer": "0.12.0",
		"is-plain-object": "5.0.0"
	},
	"devDependencies": {
		"@types/react": "16.14.0"
	},
	"sideEffects": false,
	"files": [
		"dist/**/*"
	]
