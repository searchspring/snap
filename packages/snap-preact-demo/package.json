		"@searchspring/snap-preact-components": "^0.3.1",
		"@searchspring/snap-profiler": "^0.3.1",
		"@searchspring/snap-store-mobx": "^0.3.1",
		"@searchspring/snap-toolbox": "^0.3.1",
		"@searchspring/snap-url-manager": "^0.3.1",
		"mobx": "^6.2.0",
		"mobx-react": "^7.1.0",
		"preact": "^10.5.13"
	},
