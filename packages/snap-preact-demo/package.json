{
	"name": "@searchspring/snap-preact-demo",
	"private": true,
	"version": "0.12.0",
	"description": "demo store for development and testing",
	"author": "Searchspring",
	"license": "MIT",
	"scripts": {
		"analyze": "webpack --config webpack.prod.js --analyze",
		"analyze:universal": "webpack --config webpack.prod.universal.js --analyze",
		"build": "echo 'not building demo store'",
		"build:docs": "echo 'no docs for snap-preact-demo'",
		"cypress": "cypress open --project tests",
		"dev": "kill $(lsof -ti:4444,8888); webpack serve --config webpack.dev.js",
		"format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
		"lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
		"test": "(kill $(lsof -ti:4444,8888); webpack serve --config webpack.prod.js & cypress run --project tests; r=$?; kill $(lsof -ti:4444,8888); [ \"$r\" -gt 0 ] && exit 1; exit 0)",
		"lighthouse": "lhci autorun --throttling.cpuSlowdownMultiplier=2.2"
	},
	"dependencies": {
		"@searchspring/snap-preact": "^0.12.0",
		"@searchspring/snap-preact-components": "^0.12.0",
		"deepmerge": "^4.2.2",
		"mobx": "^6.3.8",
		"mobx-react": "^7.2.1",
		"preact": "^10.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/plugin-proposal-decorators": "^7.16.4",
		"@babel/plugin-transform-react-jsx": "^7.16.0",
		"@babel/plugin-transform-runtime": "^7.16.4",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/runtime": "^7.16.3",
		"babel-loader": "^8.2.3",
		"core-js": "^3.19.3",
		"css-loader": "^6.5.1",
		"cypress": "8.5.0",
		"file-loader": "^6.2.0",
		"sass": "^1.44.0",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.65.0",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0",
		"webpack-merge": "^5.8.0"
	},
	"browserslist": [
		"last 1 version",
		"not dead",
		"ie >= 11"
	]
}
