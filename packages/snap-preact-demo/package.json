{
	"name": "@searchspring/snap-preact-demo",
	"private": true,
	"version": "0.10.0",
	"description": "demo store for development and testing",
	"author": "Searchspring",
	"license": "MIT",
	"scripts": {
		"analyze": "webpack --config webpack.prod.js --analyze",
		"build": "echo 'not building demo store'",
		"build:docs": "echo 'no docs for snap-preact-demo'",
		"build:watch": "rm -fr ./dist && webpack --config webpack.prod.js --watch --mode=production",
		"cypress": "cypress open --project tests",
		"dev": "kill $(lsof -ti:4444,8888); webpack serve --config webpack.dev.js",
		"format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
		"lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
		"test": "(kill $(lsof -ti:4444,8888); webpack serve --config webpack.prod.js & cypress run --project tests; r=$?; kill $(lsof -ti:4444,8888); [ \"$r\" -gt 0 ] && exit 1; exit 0)"
	},
	"dependencies": {
		"@searchspring/snap-preact": "^0.10.0",
		"@searchspring/snap-preact-components": "^0.10.0",
		"deepmerge": "^4.2.2",
		"mobx": "^6.3.3",
		"mobx-react": "^7.2.0",
		"preact": "^10.5.14"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-decorators": "^7.15.4",
		"@babel/plugin-transform-react-jsx": "^7.14.9",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-react": "^7.14.5",
		"@babel/runtime": "^7.15.4",
		"@prefresh/webpack": "^3.3.2",
		"babel-loader": "^8.2.2",
		"core-js": "^3.17.3",
		"css-loader": "^6.2.0",
		"cypress": "^8.3.1",
		"file-loader": "^6.2.0",
		"lighthouse": "^8.4.0",
		"node-sass": "^6.0.1",
		"sass-loader": "^12.1.0",
		"style-loader": "^3.2.1",
		"webpack": "^5.52.0",
		"webpack-bundle-analyzer": "^4.4.2",
		"webpack-cli": "^4.8.0",
		"webpack-dev-server": "^4.2.0",
		"webpack-merge": "^5.8.0"
	},
	"browserslist": [
		"last 1 version",
		"not dead",
		"ie >= 11"
	]
}
