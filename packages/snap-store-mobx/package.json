{
	"name": "@searchspring/snap-store-mobx",
	"version": "0.0.6",
	"description": "Snap MobX Store",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"author": "Searchspring",
	"license": "MIT",
	"scripts": {
		"build": "echo 'building snap-store-mobx' && rm -rf ./dist && tsc && tsc -p tsconfig.cjs.json",
		"dev": "tsc --watch",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint 'src/**/*.{js,ts,tsx}'",
		"test": "jest --coverage",
		"test:watch": "jest --coverage --watch",
		"preversion": "npm run lint && npm test && npm run build",
		"version": "standard-version",
		"postversion": "git push && git push --tags"
	},
	"dependencies": {
		"@searchspring/snap-toolbox": "^0.0.24",
		"mobx": "^6.1.8"
	},
	"devDependencies": {
		"@searchspring/snap-url-manager": "^0.0.15",
		"@types/jest": "^26.0.22",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1",
		"standard-version": "^9.1.1",
		"ts-jest": "^26.5.4",
		"typescript": "^4.2.3"
	},
	"sideEffects": false,
	"files": [
		"dist/**/*"
	]
}
